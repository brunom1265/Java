	private static String[][] testArray;
	
	public QuestionsEngine(Questions q, int aNumbers, int type){
		this.q = q;
		this.theme = q.getTheme();
	}
	
	static void startTest(){
		running = true;
		
		while(running){
			
		}
		stopTest();
	}
	
	static void stopTest(){
		running = false;
	}
	
}
