package bvsm.images;

import java.util.ArrayList;

import javax.swing.Icon;

public class GetImages {
	public Icon icon;
	
	ArrayList<Icon> imageArray = new ArrayList<Icon>();
	
	public Icon getGif(String imageName){
		icon = new Icon(getClass().getResource(imageName + ".gif"));
		return icon;
	}
	
	public Icon getImage(String imageName){
		icon = new Icon(getClass().getResource(imageName + ".jpeg"));
		return icon;
		
	}
}
